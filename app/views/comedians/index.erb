<h1>All Comedians</h1>
<% @comedians.each do |comedian| %>
<ul>
  <h2><%= comedian.name%></h2>
  <li>Age: <%= comedian.age%></li>
  <li id='specials-count-<%= comedian.id %>'>Number of Specials: <%= comedian.specials.length %></li>
  <h3>Specials</h3>
  <% comedian.specials.each do |special| %>
  <li><%= special.name %></li>
  <% end %>
</ul>
<% end %>
<h1>Average Age of All Comedians</h1>
  <p id='average-age'><%= @comedians.average_age.round %></p>
